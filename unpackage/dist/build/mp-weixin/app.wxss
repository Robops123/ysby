.r-cb .uni-checkbox-input {
  border-radius: 50%;
}

.r-cb .uni-checkbox-input.uni-checkbox-input-checked {
  border: 1px solid #ff4500;
  background-color: #ff4500;
  color: #fff !important;
}

.r-cb .uni-checkbox-input.uni-checkbox-input-checked::before {
  font-size: 18px;
}

.r-cb:not([disabled]) .uni-checkbox-input:hover {
  color: white;
  border-color: #d1d1d1;
}

.r-cb uni-checkbox {
  display: flex;
}



