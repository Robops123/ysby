
.nav{
	z-index: 999;
	background-color: #fff;
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.search-line{
	/* height: 120upx; */
	background-color: #f4f4f4;
	border-radius: 120rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	margin: 0 40rpx;
	padding: 8rpx 15rpx;
}
.search-line icon{
	display: inline-block;
		vertical-align: middle;
}
.search-line input{
	display: inline-block;
	width: 70%;
	height: 100%;
	vertical-align: middle;
	margin-left: 10rpx;
}
.main{
	padding-top: 96rpx;
}
.banner{
	border-radius: 10rpx;
	width: 100%;
	height: 320rpx;
}
.banner2{
	height: 220rpx;
	margin-top: 20rpx;
}
.banner3{
	margin: 10rpx 0 20rpx;
	height: 260rpx;
}
.title{
	margin-right: 10rpx;
	padding-right: 10rpx;
	border-right: 1px solid #f4f4f4;
}
.content icon{
	margin-right: 5rpx;
	color: #ffba2d;
}
.content{
	display: inline-block;
	vertical-align: middle;
	width: 80%;
	height: 38rpx;
}
.list{
	text-align: justify;
	margin-top: 30rpx;
}
.list-item{
	display: inline-block;
	margin:0 42rpx 26rpx 0;
	text-align: center;
}
.list-item:nth-of-type(5n){
	margin-right: 0;
}
.list-item image{
	width: 90rpx;
	height: 90rpx;
	border-radius: 50%;
}
.card{
	padding: 10rpx 8rpx;
	margin: 15rpx 0;
}
.card1{
	background-color: #fff7f5;
}
.card .more .iconfont{
	margin-left: 10rpx;
	font-size: 28rpx;
	border-radius: 50%;
	color: #ffb6b9;
	/* background-color: #; */
	vertical-align: bottom;
}
.sp-item image{
	width: 200rpx;
	height: 200rpx;
	border-radius: 5rpx;
}
.sp-item{
	margin:8rpx 2% 20rpx 0;
	display: inline-block;
	vertical-align: top;
	width: 31%;
	/* pad */
}
.sp-item .ellipsis{
	width: 200rpx;
}
.sp-item:last-child{
	margin-right: 0;
}
.hot{
	text-align: center;
}
.hot .hot-title,
.hot .hot-title::before,
.hot .hot-title::after{
	display: inline-block;
	vertical-align: middle;
}
.hot .hot-title::before{
	content: '';
	width: 80rpx;
	height: 1px;
	background-color: #aeaeae;
	margin-right: 30rpx;
}
.hot .hot-title::after{
	content: '';
	width: 80rpx;
	height: 1px;
	background-color: #aeaeae;
	margin-left: 30rpx;
}
.hot-title image{
	width: 30rpx;
	height: 30rpx;
	margin-right: 18rpx;
}
.sp2{
	text-align: justify;
	margin: 40rpx 0 20rpx;
}
.sp2 .sp-item2{
	width: 47%;
	display: inline-block;
	margin-bottom: 15rpx;
}
.sp2 .sp-item2:nth-of-type(odd){
	margin-right: 5%;
}
.sp2 .sp-item2 view{
	margin-bottom: 20rpx;
}
.sp2 image{
	margin-bottom: 10rpx;
	width: 100%;
	height: 350rpx;
}
.card2{
	background-color: #fdf2f6;
	border-radius: 8rpx;
	padding: 10rpx 18rpx;
	margin-bottom: 0;
}
.sp-item3:last-child .sp-item3-bottom{
	border-bottom: 50rpx solid #f6f1f3;
}
.sp-item3-top{
	padding: 0 30rpx;
	margin: 20rpx 0;
}
.sp-item3-top .headface{
	width: 90rpx;
	height: 90rpx;
	border-radius: 50%;
	margin: 0 15rpx;
}
/* .sp-item3-top-middle{
	width: 300upx;
	} */
.sp-item3-top-middle image{
	width: 25rpx;
	height: 25rpx;
	vertical-align: middle;
	margin-right: 5rpx;
}
.sp-item3-top-middle text text{
	color: #a9a9a9;
	margin-left: 10rpx;
}
.sp-item3-top>view{
	display: inline-block;
	vertical-align: middle;
}
.enter-button{
	color: #ff8f94;
	border: 2px solid #ff8f94;
	padding: 10rpx 15rpx;
	border-radius: 52rpx;
	float: right;
	margin-top: 19rpx;
}
.sp-item3-bottom>view{
		display: inline-block;
		vertical-align: top;
		width: 33%;
		height: 220rpx;
		position: relative;
		overflow: hidden;
		border-radius: 20rpx;
}
.sp-item3-bottom{
	padding: 0 48rpx;
	/* margin: 0 18upx; */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	border-bottom: 22rpx solid #f6f1f3;
	padding-bottom: 22rpx;
}
.sp-item3-bottom image{
	width: 100%;
	height: 100%;
}
.sp-item3-bottom .price{
		position: absolute;
		left: 0;
		border-radius:0 20rpx 0 20rpx;
		bottom: 0;
		color: white;
		padding: 5rpx 8rpx;
		background-color: #999;
		opacity: 0.8;
}
.comment image{
		width: 45rpx;
		height: 40rpx;
		display: inline-block;
		vertical-align: middle;
}
.buy image{
		width: 60rpx;
		height: 60rpx;
		padding: 10rpx;
}
.word-bottom>view{
		display: inline-block;
		vertical-align:-webkit-baseline-middle;
}

