
page{
	background-color:#f3f3f3;
}
.nav-bar{
	background-color: #fff;
		padding: 25rpx 0;
		text-align: center;
}
.nav{
		color: #afafaf;
		display: inline-block;
		width: 33%;
		box-sizing: border-box;
}
.nav.active text{
		color: #000000;
		position: relative;
}
.nav.active text::before{
		content: '';
		position: absolute;
		bottom: -10rpx;
		width: 50rpx;
		background-color: #ff7379;
		height: 2px;
		left: 50%;
		margin-left: -25rpx;
}
.distributionOrder-title{
	width: 750rpx;
	height: 90rpx;
	line-height: 90rpx;
	background: -webkit-linear-gradient(left,#ff9da1,#ff7076);
	background: linear-gradient(to right,#ff9da1,#ff7076);
	font-family: PingFang-SC-Regular;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #ffffff;	
	margin-top: 20rpx;
	text-indent:23rpx;
}
.distributionOrder-card{
	width: 750rpx;
	background-color: #FFFFFF;
	margin-top: 20rpx;
}
.distributionOrder-card-img{
	width: 100rpx;
	height: 100rpx;
	float: left;
	margin: 16rpx 23rpx 0;
	border-radius: 50%;
	background-color: #007AFF;
}
.distributionOrder-card-title{
	font-family: PingFang-SC-Regular;
	font-size: 22rpx;
	line-height: 45rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #2f2f2f;
	float: left;
	margin-top: 16rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.distributionOrder-card-zhuangtai{
	height: 80rpx;
	font-family: PingFang-SC-Regular;
	font-size: 22rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 80rpx;
	float: right;
	text-align: center;
	margin: 16rpx 28rpx 0 0;
}
.distributionOrder-card-code{
	width: 700rpx;
	font-family: PingFang-SC-Regular;
	font-size: 20rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #888888;
	margin: 41rpx auto 0;
}
.distributionOrder-card-codes{
	margin: 18rpx auto 0;
}
.distributionOrder-card-bot{
	padding: 0 20rpx;
	border-top: 2rpx solid #ededed;
	margin-top: 28rpx;
}
.distributionOrder-card-bot-price{
	display: inline-block;
	height: 100%;
	font-family: PingFang-SC-Regular;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 76rpx;
	letter-spacing: 2rpx;
	color: #181818;
	margin-right: 20rpx;
}
.distributionOrder-card-bot-tishi{
	display: inline-block;
	height: 100%;
	font-family: PingFang-SC-Regular;
	font-size: 20rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 80rpx;
	letter-spacing: 2rpx;
	color: #3f3f3f;
}
.margin-left{
	margin-left: 20rpx;
}

